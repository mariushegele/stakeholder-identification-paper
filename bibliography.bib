	keywords = {Computer science, formal specification, Design engineering, Educational institutions, Engineering management, Human computer interaction, Informatics, Information systems, Performance analysis, requirements engineering process, scenario-based requirements, Software engineering, stakeholder identification, systems analysis, Systems engineering and theory},
